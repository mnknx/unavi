{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["build/**", "dist/**"]
    },
    "client#build": {
      "dependsOn": ["^build", "generate"],
      "outputs": [".next/**"],
      "env": [
        "DATABASE_URL",
        "DISABLE_PWA",
        "ETH_PROVIDER",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "NEXT_PUBLIC_AXIOM_INGEST_ENDPOINT",
        "NEXT_PUBLIC_CDN_ENDPOINT",
        "NEXT_PUBLIC_CRYPTOAVATARS_API_KEY",
        "NEXT_PUBLIC_DEFAULT_HOST",
        "NEXT_PUBLIC_DEPLOYED_URL",
        "NEXT_PUBLIC_DOCS_URL",
        "NEXT_PUBLIC_HAS_DATABASE",
        "NEXT_PUBLIC_HAS_GOOGLE_OAUTH",
        "NEXT_PUBLIC_HAS_S3",
        "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID",
        "NODE_ENV",
        "PLANETSCALE",
        "S3_ACCESS_KEY_ID",
        "S3_BUCKET",
        "S3_ENDPOINT",
        "S3_REGION",
        "S3_SECRET",
        "VERCEL_URL",
        "VERCEL"
      ]
    },
    "host#build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["build/**"],
      "env": [
        "MEDIASOUP_ANNOUNCED_IP",
        "MEDIASOUP_LISTEN_IP",
        "RTC_MAX_PORT",
        "RTC_MIN_PORT",
        "SSL_CERT",
        "SSL_KEY"
      ]
    },
    "lint": {},
    "generate": { "cache": false },
    "dev": { "cache": false, "persistent": true }
  }
}
